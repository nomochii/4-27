<div class="container">
  <div class="row">
    <div class="col-md-3">

      <%= render 'books/list', newbook: @newbook,user: @user %>
    </div>


    <div class="col-md-8 offset-md-1">
      <h2>Userss</h2>
      <table class="teble">
        <thead>
    		  <tr>
    			  <th>Image</th>
    			  <th>name</th>
    	  	</tr>
    	  </thead>

      <% @users.each do |user| %>
        <tbody>
          <td>
            <%= link_to user_path(user.id) do %>
              <%= image_tag user.get_profile_image(100,100) %>
            <% end %>
          </td>
          <td><%= user.name %></td>
          <td><%= link_to "show",user_path(user.id)%></td>
        </tbody>
      </table>
      <% end %>

    </div>
  </div>
</div>
